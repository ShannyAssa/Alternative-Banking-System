<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox spacing="13.0" style="-fx-background-color: #071D4C;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.header.HeaderController">
   <children>
      <Label text="Current Yaz:" textFill="WHITE" HBox.hgrow="SOMETIMES">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
      <TextField fx:id="currentYazField" disable="true" HBox.hgrow="ALWAYS" />
      <Label layoutX="15.0" layoutY="15.0" text="System Mode:" textFill="WHITE">
         <padding>
            <Insets left="20.0" />
         </padding>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="systemModeField" disable="true" layoutX="92.0" layoutY="15.0" HBox.hgrow="ALWAYS" />
      <Label fx:id="helloUserLabel" text="Hello user!" textFill="WHITE">
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
