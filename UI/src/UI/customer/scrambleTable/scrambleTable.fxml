<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<ScrollPane fx:id="scrambleTableComponent" fitToHeight="true" fitToWidth="true" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UI.customer.scrambleTable.ScrambleTableController">
   <content>
      <TableView fx:id="scrambleTable" editable="true">
         <columns>
            <TableColumn fx:id="loanIdCol" prefWidth="75.0" text="Loan ID" />
            <TableColumn fx:id="categoryCol" prefWidth="75.0" text="Category" />
            <TableColumn fx:id="statusCol" prefWidth="75.0" text="Status" />
            <TableColumn fx:id="principalColumn" prefWidth="86.0" text="Principal" />
            <TableColumn fx:id="paymentIntervalCol" prefWidth="126.0" text="Payment Interval" />
            <TableColumn fx:id="interestRateCol" prefWidth="106.0" text="Interest Rate" />
            <TableColumn fx:id="totalAmountCol" prefWidth="96.0" text="Total Amount" />
            <TableColumn fx:id="selectLoanCol" prefWidth="96.0" text="Select Loan" />
         </columns>
      </TableView>
   </content>
</ScrollPane>
